  <rdf:Description rdf:about="file:///home/tai/src/perlmods/XRD-Parser/Changes.ttl">
    <dc:creator rdf:resource="http://tobyinkster.co.uk/#i"/>
    <dc:description xml:lang="en">Revision history for Perl extension XML::Atom::OWL.</dc:description>
    <dc:subject rdf:resource="http://search.cpan.org/dist/XRD-Parser/#project"/>
    <dc:title>Changes</dc:title>
  </rdf:Description>
  <Version rdf:about="http://search.cpan.org/dist/XRD-Parser/#v_0_01">
    <file-release rdf:resource="http://backpan.perl.org/authors/id/T/TO/TOBYINK/XRD-Parser-0.01.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.01</revision>
    <rdfs:label xml:lang="en">First release</rdfs:label>
  </Version>
  <Version rdf:about="http://search.cpan.org/dist/XRD-Parser/#v_0_02">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Support for host-meta's &lt;hm:Host/&gt; element as a way to set subjects.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Implement &lt;Alias/&gt; element as an owl:sameAs-like predicate rather than simply duplicating data.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Support &lt;Link template/&gt; attribute via a special datatype and special predicate URI scheme.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Support for IANA keywords in &lt;Link rel/&gt; attribute.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Map XRD &lt;Expires/&gt; element to the RDF URI &lt;http://ontologi.es/xrd#expires&gt;.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:versus rdf:resource="http://search.cpan.org/dist/XRD-Parser/#v_0_01"/>
      </rdf:Description>
    </dcs:changeset>
    <file-release rdf:resource="http://backpan.perl.org/authors/id/T/TO/TOBYINK/XRD-Parser-0.02.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.02</revision>
  </Version>
  <Version rdf:about="http://search.cpan.org/dist/XRD-Parser/#v_0_03">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Provide an XRD::Parser-&gt;hostmeta($uri) shortcut constructor.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Send proper HTTP Accept header when requesting XRD files.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Prefer namespace-aware DOM functions.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:versus rdf:resource="http://search.cpan.org/dist/XRD-Parser/#v_0_02"/>
      </rdf:Description>
    </dcs:changeset>
    <file-release rdf:resource="http://backpan.perl.org/authors/id/T/TO/TOBYINK/XRD-Parser-0.03.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.03</revision>
  </Version>
  <Version rdf:about="http://search.cpan.org/dist/XRD-Parser/#v_0_04">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Support http://host-meta.net/xrd/1.0 and http://host-meta.net/ns/1.0 as alternatives.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Don't reify &lt;Link&gt; elements multiple times for multiple &lt;Property&gt; children.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Parse &lt;Property&gt; children of &lt;Link template&gt; elements.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:versus rdf:resource="http://search.cpan.org/dist/XRD-Parser/#v_0_03"/>
        <rdfs:comment xml:lang="en">And other minor bugfixes.</rdfs:comment>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2009-12-16</dc:issued>
    <file-release rdf:resource="http://backpan.perl.org/authors/id/T/TO/TOBYINK/XRD-Parser-0.04.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.04</revision>
  </Version>
  <Version rdf:about="http://search.cpan.org/dist/XRD-Parser/#v_0_05">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Provide an alternative method of handling &lt;Property&gt; within &lt;Link&gt;: RDFS subproperties.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Allow both methods of handling &lt;Property&gt; within &lt;Link&gt; to be toggled independently.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">host_uri and template_uri utility functions.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Update>
            <rdfs:label xml:lang="en">Switch to my new build system.</rdfs:label>
          </dcs:Update>
        </dcs:item>
        <dcs:item>
          <dcs:Update>
            <rdfs:label xml:lang="en">Synchronise callback functionality with RDF::RDFa::Parser 0.30.</rdfs:label>
          </dcs:Update>
        </dcs:item>
        <dcs:item>
          <dcs:Update>
            <rdfs:label>Copyright 2010</rdfs:label>
          </dcs:Update>
        </dcs:item>
        <dcs:versus rdf:resource="http://search.cpan.org/dist/XRD-Parser/#v_0_04"/>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-02-02</dc:issued>
